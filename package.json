{
  "name": "sunrise",
  "version": "1.0.0",
  "description": "a electron-vue project",
  "author": "'Sun <'275050943@qq.com'>",
  "private": true,
  "main": "./dist/electron/source/main.js",
  "scripts": {
    "build:clean": "cross-env BUILD_TARGET=clean node build/pc/build.js",
    "build:web": "node build/web/build.js",
    "build:win": "node build/pc/build.js && electron-builder",
    "build:unwin": "node build/pc/build.js && electron-builder --dir",
    "build:win:t1": "npm run build:unwin && cross-env Template=t1 node build/pc/winBaleTool/main.js",
    "build:win:t2": "npm run build:unwin && cross-env Template=t2 node build/pc/winBaleTool/main.js",
    "build:linux": "npm run build:clean && node build/pc/build.js && electron-builder --linux",
    "dev:pc": "node build/pc/dev-runner.js",
    "dev": "webpack-dev-server --inline --progress --config build/web/webpack.dev.conf.js"
  },
  "build": {
    "productName": "ele-vue",
    "appId": "org.simulatedgregbuild",
    "directories": {
      "output": "dist/electron"
    },
    "files": [
      "dist/electron/source/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons",
      "target": [
        "tar.gz"
      ]
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "codemirror": "^5.48.0",
    "driver.js": "^0.9.7",
    "echarts": "^4.2.1",
    "element-ui": "^2.10.0",
    "font-awesome": "^4.7.0",
    "js-cookie": "^2.2.0",
    "jsonlint": "^1.6.3",
    "localforage": "^1.7.2",
    "mavon-editor": "^2.7.4",
    "md5": "^2.2.1",
    "v-charts": "^1.19.0",
    "vue": "^2.5.2",
    "vue-count-to": "^1.0.13",
    "vue-electron": "^1.0.6",
    "vue-filter": "^0.2.5",
    "vue-i18n": "^8.0.0",
    "vue-progressbar": "^0.7.5",
    "vue-router": "^3.0.1",
    "vue2-animate": "^2.0.0",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.5.4",
    "xe-utils": "^1.8.24"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^8.2.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
